{
  "type": "object",
  "comment": "Blank",
  "properties": {
    "monitor": {
      "type": "action"
    },
    "unassigned": {
      "type": "action"
    },
    "assigned": {
      "type": "action"
    },
    "Application": {
      "type": "action",
      "control": "property.innerText",
      "selector": "span[class='c']",
      "selectContains": "Application"
    },
    "Approval": {
      "type": "action",
      "control": "property.innerText",
      "selector": "span[class='c']",
      "selectContains": "Approval"
    },
    "Personal Information": {
      "type": "action",
      "control": "property.innerText",
      "selector": "span[class='c']",
      "selectContains": "Personal Information"
    },
    "isHome": {
      "type": "identifier",
      "control": "exists",
      "selector": "img[src='images/mainposition.gif']"
    },
    "isAppSelected": {
      "type": "identifier",
      "control": "exists",
      "selector": "div.nodeon",
      "selectContains": "Application"
    },
    "isPersonalInfoSelected": {
      "type": "identifier",
      "selector": "div.nodeon",
      "selectContains": "Personal Information"
    }
  },
  "id": "homeiconcolorblocks",
  "connect": [
    {
      "match": [
        "isHome"
      ],
      "event": "homeiconcolorblocks"
    }
  ]
}
